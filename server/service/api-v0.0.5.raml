      responses:
        200:
          body:
            application/json:
              example: |
                {
                  "result":0(失败),1(成功),
                  "errorMsg": 失败原因
                }
        401:
  /auth/deleteMessage: 
    description: 删除消息
    post:
      queryParameters:
        token:
          displayName: token
          type: string 
          example: sdfs234rrjkn2IFO
          required: true
