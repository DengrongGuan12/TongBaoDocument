#%RAML 0.8
title: tongbao
version: 0.0.3
#---method---
#--string 类型的一定要考虑限制长度 ,这个长度会等数据库建完之后给出; 401错误用来表示未登陆状态下访问受限的url状态码--
/user:
  /register:
    description: 注册
    post:
      queryParameters:
        phoneNumber:
          displayName: phoneNumber
          type: string
          example: 15851813132
          required: true
          description: 必须为正确的手机号格式
        password:
          displayName: password
          type: string
          example: abc123
          required: true
          description: 限定最小长度,大于等于8
        type:
          displayName: type
          type: integer
          example: 0
          required: true
          description: 0表示货主, 1表示司机
      responses:
        200:
          body:
            application/json:
              example: |
                {
                  "result":0(失败)1(成功),
                  "errorMsg":(表示失败的原因，比如手机号已经注册)
                }
  /login:
    description: 登陆
    post:
      queryParameters:
        phoneNumber:
          displayName: phoneNumber
          type: string
          example: 15851813132
          required: true
        password:
          displayName: password
          type: string
          example: abc123
          required: true
        type:
          displayName: type
          type: integer
          example: 0
          required: true
          description: 0货主 1司机
      responses:
        200:
          body:
            application/json:
              example: |
                {
                  "result":(失败)1(成功),
                  "data"(只有登陆成功才会有):{"nickName":"夏明","iconUrl":"abc.com","point":23(积分),"money":34(账户余额),"token":用户识别的token,"id":1(用户id)}
                }
  /hasRegister:
    description: 判断手机号有没有被注册过
    post:
      queryParameters:
        phoneNumber:
          displayName: phoneNumber
          type: string
          example: 15851813132
          required: true
          description: 必须为正确的手机号格式
      responses:
        200:
          body:
            application/json:
              example: |
                {
                  "result":0(未注册)1(已注册)
                }
  /uploadPicture:
    description: 上传图片
    post:
      queryParameters:
        file:
          displayName: file
          type: file
          example: 
          required: true
          description: 这个使用post提交的文件，与在网页上上传文件类似
      responses:
        200:
          body:
            application/json:
              example: |
                {
                  "result": 0(失败)1(成功),
                  "data": "url"(图片的url),
                  "errorMsg": 失败的原因
                }
  /auth/modifyNickName:
    description: 修改个人昵称
    post:
      queryParameters:
        token:
          displayName: token
          type: string 
          example: sdfs234rrjkn2IFO
          required: true
          description: 用于识别用户
        nickName:
          displayName: nickName
          type: string
          example: 小花
          required: true
          description: 修改后的用户名
      responses:
        200:
          body:
            application/json:
              example: |
                {
                  "result": 0(失败)1(成功),
                  "errorMsg":失败的原因
                }
        401:
  /auth/modifyPassword:
    description: 修改密码
    post:
      queryParameters:
        token:
          displayName: token
          type: string
          example: sdfs234rrjkn2IFO
          required: true
          description: 用于识别用
        oldPassword:
          displayName: oldPassword
          type: string
          example: wree54654
          required: true
          description: 旧密
        newPassword:
          displayName: newPassword
          type: string
          example: fsdf23re
          required: true
          description: 新密码,长度大于等于8小于等于255
      responses:
        200:
          body:
            application/json:
              example: |
                {
                  "result":0(失败)1(成功),
                  "errorMsg":失败原因
                }
        401:
  /auth/modifyIcon:
    description: 修改个人头像
    post:
      queryParameters: 
        token:
          displayName: token
          type: string 
          example: sdfs234rrjkn2IFO
          required: true
          description: 用于识别用户
        iconUrl:
          displayName: iconUrl
          type: string 
          example: abc.com
          required: true
          description: 图片的url
      responses:
        200:
          body:
            application/json:
              example: |
                {
                  "result":0(失败)1(成功),
                  "errorMsg":失败原因
                }
        401:
  /auth/getContacts:
    description: 获取联系人列表
    post:
      queryParameters:
        token:
          displayName: token
          type: string 
          example: sdfs234rrjkn2IFO
          required: true
          description: 用于识别用户
      responses:
        200:
          body:
            application/json:
              example: |
                {
                  "result":0(失败),1(成功),
                  "data":[{"id":1,"nickName":昵称,"phoneNum":12345,"type":(用户类型)0表示货主 1表示司机},
                          {"id":2,"nickName":昵称,"phoneNum":12345,"type":(用户类型)0表示货主 1表示司机},{}...] 
                } 
        401:
  /auth/getContactDetail:
    description: 获取联系人
    post:
      queryParameters:
        token:
          displayName: token
          type: string 
          example: sdfs234rrjkn2IFO
          required: true
          description: 用于识别用户
        id:
          displayName: id
          type: integer 
          example: 1
          required: true
          description: 用于获取用户详情
      responses:
        200:
          body:
            application/json:
              example: |
                {
                  "result":0(失败),1(成功),
                  "data": {"id":1(用户id),"phoneNum":12345,"type":(用户类型)0表示货主 1表示司机,
                          "nickName":昵称,"sex":0男1女,"regTime":(注册时间)"2015-11-11 00:00:00"}
                }
        401:
  /auth/getMyMessages:
    description: 获取我的消息列表
    post:
      queryParameters:
        token:
          displayName: token
          type: string 
          example: sdfs234rrjkn2IFO
          required: true
          description: 用于识别用户
      responses:
        200:
          body:
            application/json:
              example: |
                {
                  "result":0(失败),1(成功),
                  "data":[{"id":1,"type"(类型):0:订单被抢到（通知货主）1:订单完成（通知司机）2: 正在进行的订单被取消(通知司机)3：其他消息,
                            "content":消息内容,"hasRead"(是否已读):0未读,1已读,"time"(时间):消息产生的时间,
                            "objectId":type为012时表示订单id},{}]
                  "errorMsg": 失败原因
                }
        401:
  /auth/readMessage:
    description: 读消息
    post:
      queryParameters:
        token:
          displayName: token
          type: string 
          example: sdfs234rrjkn2IFO
          required: true
          description: 用于识别用户
        id:
          displayName: id
          type: integer
          example: 1
          required: true
