                    "errorMsg":失败原因
                  }
          401:
  /auth/placeOrder:
    description: 下单
    post:
      queryParameters:
        token:
          displayName: token
          type: string 
          example: sdfs234rrjkn2IFO
          required: true
          description: 用于识别用户
        addressFrom:
          displayName: addressFrom
          type: string
          example: 汉口路22号
          required: true
          description: 发货地
        addressFromLat:
          displayName: addressFromLat
          type: string
          example: 38.8
          required: true
          description: 发货地纬度,应该是double类型
        addressFromLng:
          displayName: addressFromLng
          type: string
          example: 128.8
          required: true
          description: 发货地经度，应该是double类型
        addressTo:
          displayName: addressTo
          type: string 
          example: 汉口路55号
          required: true
          description: 收货地
