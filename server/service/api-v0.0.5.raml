      responses:
        200:
          body:
            application/json:
              example: |
                {
                  "result":0(失败),1(成功),
                  "errorMsg": 失败原因
                }
        401:
  /auth/showAccount:
    description: 查看账单
    post:
      queryParameters:
        token:
          displayName: token
          type: string 
          example: sdfs234rrjkn2IFO
          required: true
          description: 用于识别用户
      responses:
        200:
          body:
            application/json:
              example: |
                {
                  "result":0(失败),1(成功)
                  "data":[
                      {
                      "type":(账单类型)0表示充值 1表示提现 2表示支付 3表示退款 4表示到帐, "time":(交易时间), "money":12(金额),
                      "order"(只有type为2,3,4时才有该值):{"id":1,"addressFrom":开始地点,"addressTo":终止地点}
                      },
                      {},...]
                } 
        401:
  /getAllTruckTypes:
    description: 获取所有车辆类型
    post:
      responses:
        200:
          body:
            application/json:
              example: |
                {
                  "result": 0(失败),1(成功),
                  "errorMsg": 失败的原因 ,
